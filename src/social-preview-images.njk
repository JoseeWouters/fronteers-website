---
pagination:
  data: collections.published_posts_after_2024 
  size: 1
  alias: preview
permalink: "/assets/images/social-preview-images/{{ preview.data.date | postDate }}-{{ preview.data.title | slug }}-preview-{{ preview.data.locale }}.svg" 
eleventyExcludeFromCollections: true

---
<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="1200" height="628" viewBox="0 0 1200 628" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">

    {% if preview.data.title != undefined %}
        {% set titleInLines = preview.data.title | splitlines %}
        {% set numberOfLines = titleInLines.length %}
    {% endif %}

    {% if numberOfLines == 1 %}
        {% set verticalStartingPoint = 350 %}
    {% elif numberOfLines == 2 %}
        {% set verticalStartingPoint = 250 %}
    {% elif numberOfLines == 3 %}
        {% set verticalStartingPoint = 200 %}
    {% elif numberOfLines == 4 %}
        {% set verticalStartingPoint = 250 %}
    {% elif numberOfLines == 5 %}
        {% set verticalStartingPoint = 250 %}
    {% else %}
        {% set verticalStartingPoint = 250 %}
    {% endif %}

    {% set tags = preview.data.tags | default([]) %}
    {% if "posts" in tags or "activities" in tags %}
        <rect width="100%" height="100%" fill="#b7b7fd" />
    {% else %}
        <rect width="100%" height="100%" fill="#fe796a" />
    {% endif %}

    <text id="text" font-family="Fabrik, Roboto, sans-serif" font-size="80" font-weight="bold"> 
        {% if "members" in tags %}
            {% for line in titleInLines %}
                <tspan x="73" y="300">{{ line }}</tspan>
            {% endfor %}
            {% if preview.data.jobtitle %}
                 <tspan x="73" y="400" font-size="60" font-weight="normal">{{ preview.data.jobtitle }}</tspan>
            {% endif %}
        {% else %}
            {% for line in titleInLines %}
                <tspan x="73" y="{{verticalStartingPoint + loop.index * 80}}">{{ line }}</tspan>
            {% endfor %}
        {% endif %}
    </text>

    {% if "posts" in tags %}
        <rect width="1200" height="96" fill="black" x="0" y="535" />
        <text font-family="Roboto, sans-serif" font-size="30" font-weight="bold">
            <tspan x="73" y="590" fill="white">
                {% if preview.data.date %}
                    {% if preview.data.locale == "nl" %}
                        {{ preview.data.date | localizedDate('nl') | upper }}
                    {% else %}
                        {{ preview.data.date | readablePostDate | upper }}
                    {% endif %}
                {% endif %}  

                {% if preview.data.author %} - {{ preview.data.author | upper }}{% endif %}</tspan>
        </text>
    {% elif "activities" in tags %}
        <rect width="1200" height="96" fill="black" x="0" y="535" />
        <text font-family="Roboto, sans-serif" font-size="30" font-weight="bold">
            <tspan x="73" y="590" fill="white">
                {% if preview.data.location %}
                    {{ preview.data.location | upper }}
                {% endif %}

                {% if preview.data.eventdate %}
                    {% if preview.data.location %} - {% endif %}
                    {% if preview.data.locale == "nl" %}
                        {{ preview.data.eventdate | localizedDate('nl') | upper }}.
                    {% else %}
                        {{ preview.data.eventdate | readablePostDate | upper }}.
                    {% endif %}
                {% endif %} 

                {% if preview.data.locale == "nl" %}
                    CHECK ONZE WEBSITE VOOR MEER INFO!
                {% else %}
                    CHECK OUR WEBSITE FOR MORE INFO!
                {% endif %}
            </tspan>
        </text>
    {% elif "members" in tags %}
        <rect width="1200" height="96" fill="black" x="0" y="535" />
        <text font-family="Roboto, sans-serif" font-size="30" font-weight="bold">
            <tspan x="73" y="590" fill="white">
                {% if preview.data.freelancer %}
                    FREELANCER
                    {% if preview.data.freelancer.availability == true %}
                        {% if preview.data.locale == "nl" %}
                            (BESCHIKBAAR)
                        {% else %}
                            (AVAILABLE)
                        {% endif %}
                        
                    {% endif %}
                {% endif %}

                {% if preview.data.lookingForEmployment and preview.data.lookingForEmployment.availability == true %}
                    {% if preview.data.locale == "nl" %}
                        BESCHIKBAAR VOOR WERK
                    {% else %}
                        AVAILABLE FOR WORK
                    {% endif %}
                {% endif %}

                {% if preview.data.freelancer or preview.data.lookingForEmployment %}
                    Â· 
                {% endif %}

                {% if preview.data.mentor %}
                    {% if preview.data.locale == "nl" %}
                        BESCHIKBAAR ALS MENTOR
                    {% else %}
                        AVAILABLE AS MENTOR
                    {% endif %}
                {% endif %}
            </tspan>
        </text>
    {% endif %}

{% if "members" in tags %}
    {{ preview.data.graphic.src }}
{% endif %}


