<section data-partial="homepage/proud-members" class="inner-wrapper proud-members">
    {%- include partials/utility/dynamic-headerlevel level: headerlvl title: partialTitle -%}
    <ul class="desktop-columns">
        {%- for member in members limit: 3 -%}
            <li class="list-item">
                {%- include partials/member index: forloop.index -%}
            </li>
        {%- endfor -%}
    </ul>

    <div class="button-wrapper">
        <a href="{{ translations[locale].blockTitles.ourMembersPath }}" class="button button-parentheses">
            {{ translations[locale].blockTitles.viewOurMembers }}
        </a>
    </div>
</section>
